{
    "parser": "babel-eslint",
    "extends": "standard",
    "plugins": [
      "react"
    ],
    "rules": {
      "key-spacing": [2, {
        "singleLine": {
          "beforeColon": false,
          "afterColon": true
        },
        "multiLine": {
          "align": "colon",
          "beforeColon": true,
          "afterColon": true
        }
      }],
      "react/self-closing-comp": 1
    }
}
